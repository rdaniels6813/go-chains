{
  "go.useLanguageServer": true,
  "go.lintTool": "golangci-lint",
  "[go]": {
    "editor.snippetSuggestions": "none",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "files.associations": {
    "keychain.h": "c",
    "initializer_list": "c",
    "type_traits": "c",
    "vector": "c",
    "xstring": "c",
    "xutility": "c",
    "iosfwd": "c"
  }
}
